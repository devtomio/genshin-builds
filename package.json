{
  "name": "genshin-builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=./db/schema.prisma && next build",
    "lint": "next lint",
    "analize": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "type-check": "tsc",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./db/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./db/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@aws-sdk/s3-request-presigner": "^3.529.1",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@freestar/pubfig-adslot-react-component": "^3.6.1",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@nanostores/persistent": "^0.10.1",
    "@nanostores/react": "^0.7.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@planetscale/database": "^1.16.0",
    "@prisma/adapter-planetscale": "^5.10.2",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-table": "^8.13.2",
    "@uiw/react-md-editor": "^4.0.3",
    "@vercel/og": "^0.6.2",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "leaflet": "^1.9.4",
    "nanostores": "^0.10.0",
    "negotiator": "^0.6.3",
    "next": "^14.1.3",
    "next-auth": "^4.24.7",
    "next-mdx-remote": "^4.4.1",
    "next-pwa": "^5.6.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.0.1",
    "react-lazy-load-image-component": "^1.6.0",
    "react-leaflet": "^4.2.1",
    "react-tooltip": "^5.26.3",
    "react-wrap-balancer": "^1.1.0",
    "remark-gfm": "^3.0.0",
    "schema-dts": "^1.1.2",
    "sonner": "^1.4.3",
    "swr": "^2.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.3",
    "@next/env": "^14.1.3",
    "@types/leaflet": "^1.9.8",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.11.26",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-lazy-load-image-component": "^1.6.3",
